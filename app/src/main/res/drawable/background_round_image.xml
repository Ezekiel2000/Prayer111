        android:top="4dp"
        android:right="4dp"
        android:left="4dp"
        android:bottom="4dp" >

        <shape
            android:shape="oval">
            <solid andoird:color="#c1c1c1" />
        </shape>
    </item>
    <item>
        <shape
            android:shape="oval">
            <stroke android:width="2dp"
                    android:color="#515151" />
        </shape>
    </item>
</layer-list>
